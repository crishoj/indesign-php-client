{
    "name": "davidnjuko/indesign-client",
    "description": "InDesign Server PHP Client",
    "keywords": [
        "client",
        "indesign"
    ],
    "homepage": "http://davidribes.fr",
    "type": "library",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "David Ribes",
            "homepage": "http://davidribes.fr/"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-soap": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "sempro/phpunit-pretty-print": "^1.0",
        "nunomaduro/collision": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "InDesignClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InDesignClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
